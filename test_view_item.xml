<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="test_view_item.xml" verbose="1" parallel="methods" thread-count="6">
    <listeners>
        <listener class-name="objects.OnFailTestListener"/>
    </listeners>
    <test name="test_view_item_uk">
        <parameter name="local" value="uk"/>
        <groups>
            <run>
                <include name="UK"/>
            </run>
        </groups>
        <classes>
            <class name="tests.ViewItemTest"/>
        </classes>
    </test>
    <test name="test_view_item_au">
        <parameter name="local" value="au"/>
        <groups>
            <run>
                <include name="AU"/>
            </run>
        </groups>
        <classes>
            <class name="tests.ViewItemTest"/>
        </classes>
    </test>
    <test name="test_view_item_de">
        <parameter name="local" value="de"/>
        <groups>
            <run>
                <include name="DE"/>
            </run>
        </groups>
        <classes>
            <class name="tests.ViewItemTest"/>
        </classes>
    </test>
    <test name="test_view_item_us">
        <parameter name="local" value="us"/>
        <groups>
            <run>
                <include name="US"/>
            </run>
        </groups>
        <classes>
            <class name="tests.ViewItemTest"/>
        </classes>
    </test>
</suite>